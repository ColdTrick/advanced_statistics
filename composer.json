{
	"name": "coldtrick/advanced_statistics",
	"description": "Show more statistics about you Elgg installation",
	"homepage": "https://www.coldtrick.com",
	"type": "elgg-plugin",
	"keywords": ["elgg", "plugin", "statistics"],
	"license": "GPL-2.0-only",
	"support": {
		"source": "https://github.com/ColdTrick/advanced_statistics",
		"issues": "https://github.com/ColdTrick/advanced_statistics/issues"
	},
	"require": {
		"npm-asset/chart.js": "~4.4.0"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	],
	"config": {
		"fxp-asset": {
			"enabled": false
		}
	},
	"conflict": {
		"elgg/elgg": "<6.0"
	}
}
